
<Stacker cells=5 width=100% height=100% gutter_width=0px>
    <Stacker cells=10 height=100% width=100% direction=StackerDirection::Vertical gutter_width=0px >
        for i in 0..10 {
            <Rectangle width=100% height=100% fill={Color::rgb(0.0, 1.0 / (10.0-i), 0.5)} />
        }
    </Stacker>
    for i in 0..4 {
        <Rectangle width=100% height=100% fill={Color::rgb(1.0 / (5.0-i), 0.0, 0.5)} />
    }
</Stacker>

