for data in self.slot_children_data {
	<Group
		x={data.x}
		y={data.y}
		width={self.width}
		scale_x={data.scale}
		scale_y={data.scale}
	>
	    slot(data.index)
	</Group>
}

@settings {
    @mount: on_mount
    @wheel: handle_wheel
    @pre_render: update,
    @touch_move: touch_move,
    @touch_start: touch_start,
    @touch_end: touch_end,
}

