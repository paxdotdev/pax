<Ellipse class=ball />

@settings {
    .ball {
        anchor_x:50%,
        anchor_y:50%,
        fill: {
            hsl(magnitude * 4 + 280, 75%, 50%)
        },
        stroke: {
            color: WHITE,
            width: 5px,
        }
        width: {magnitude * 7}
        height: {magnitude * 7}
    }
}
