<Group>
    <Rectangle class=one_rect
         fill={Fill::Solid(Color::hlc(ticks, 75.0, 150.0))}
    />
</Group>

@settings {
    .one_rect {
        x: 50%
        y: 50%
        anchor_x: 50%
        anchor_y: 50%
        width: 50%
        height: 50%
    }
}

@handlers {
    pre_render: handle_pre_render
}