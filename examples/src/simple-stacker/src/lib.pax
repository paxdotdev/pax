
for (v, i) in self.vec {
    <Rectangle x={Size::Pixels(v)} width=100px fill={Fill::Solid(Color::rgb(0.5, 0.5, 1.0 / (10.0 - 0.01*v)))} />
}

for i in 0..2 {
    <Rectangle x={Size::Percent(i*50 + 10)} width=10px fill={Fill::Solid(Color::rgb(0.9, 0.5, i/2))} />
}

@handlers{
    mount: handle_mount
	pre_render: handle_pre_render
}

