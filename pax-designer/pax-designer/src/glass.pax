
for i in self.control_points {
    <Rectangle class=control_point x={(i.x)px} y={(i.y)px} />
}


@settings {
    .control_point {
        stroke: {
            color: {Color::rgb(1, 0, 0)}
            width: 1px
        }
        fill: {
            Fill::Solid(Color::rgba(0,0,0,0))
        }
        width: 7px
        height: 7px
    }
}