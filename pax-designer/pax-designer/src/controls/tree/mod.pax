for (obj, i) in self.tree_objects {
	<TreeObj
		 ind={obj.ind}
		 y={(40 + i*42)px}
		 x={(obj.indent_level*6)%}
		 is_selected={obj.is_selected}
		 height=40px
		 name={obj.name}
		 is_not_leaf={obj.is_not_leaf}
		 is_collapsed={obj.is_collapsed}
		 width={(100.0 - obj.indent_level*6)%}
		 image_path={obj.image_path}
		 is_not_dummy={obj.is_not_dummy}
	/>
}
@settings {
	@mount: on_mount
	@pre_render: pre_render
}
