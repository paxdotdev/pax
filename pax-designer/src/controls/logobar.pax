<Image id=logo source=ImageSource::Url("assets/pax-designer-white.png") @click=handle_logo_click />
<Link id=face url="https://docs.pax.dev/">
    <Image source=ImageSource::Url("assets/zface.png")/>
</Link>

@settings {
    #logo {
        width: 75px,
        height: 75px,
        x: 16px,
    }

    #face {
        width: 35px,
        height: 35px,
        anchor_x: 100%,
        anchor_y: 50%,
        x: {100% - 15px}
        y: 50%
    }
}
