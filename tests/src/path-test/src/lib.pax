<Path stroke={color: Color::RED, width: 5px}>
    <PathPoint x=0.0 y=0.0/>
    if self.jump {
        <PathLine/>
        <PathPoint x=100.0 y=400.0/>
    }
    <PathLine/>
    <PathPoint x=300.0 y=300.0/>
    <PathCurve x=500.0 y=300.0/>
    <PathPoint x=300.0 y=100.0/>
    <PathClose/>
</Path>

@settings {
    @click: self.toggle
}
